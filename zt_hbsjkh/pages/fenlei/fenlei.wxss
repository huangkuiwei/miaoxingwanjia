}

.fenlei_content_head_item {
  display: inline-block;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  padding-right: 17rpx;
  padding-left: 17rpx;
  background: rgba(240, 239, 239, 1);
  border-radius: 15rpx;
  font-size: 24rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(96, 96, 96, 1);
  /* width: fit-content; */
