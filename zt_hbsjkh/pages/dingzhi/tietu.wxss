.tietu_container{
  width: 750rpx;
  height: 100%;
  position: absolute;
  top: 0rpx;
  background-color: #F3F3F3;
  margin-bottom: 130rpx;
  overflow: hidden;
  z-index: 999;
}
.tietu_title_title{
  width: 750rpx;
  height: 60rpx;
  overflow: hidden;
  padding-left: 5rpx;
  padding-right: 5rpx;
  white-space: nowrap;
  border-bottom: solid 1rpx #F3F3F3;
  position: relative;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.tietu_title_title_item{
  height: 29rpx;
  line-height: 29rpx;
  color: #787878;
  margin-left: 34rpx;
  margin-right: 54rpx;
  font-size: 29rpx;
  display: inline-block;
}
.active{
  border-bottom: 6rpx solid #FFDD02;
}
.tietu_title_content{
  width: 705rpx;
  position: absolute;
  left: 40rpx;
  top: 60rpx;
  bottom: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.tietu_title_content_item{
  width: 200rpx;
  margin-top: 30rpx;
  margin-right: 35rpx;
}
.tietu_foot{
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size:30rpx;
  font-family:SimHei;
  font-weight:400;
  color:#565656;
  background-color: #FFF602;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
}