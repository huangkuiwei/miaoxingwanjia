}
.huiyuan_item text{  
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.huiyuan_list{
  width: 690rpx;
  height: 632rpx;
}