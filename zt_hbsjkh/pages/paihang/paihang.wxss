.container{
  width: 750rpx;
  min-height: 100%;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  background-color: rgb(239, 239, 239);
  margin-bottom: 120rpx;
}
.head_view{
  width: 750rpx;
  height: 495rpx;
  position: relative;
  left: 0rpx;
  top: 0rpx;
  margin-bottom: 20rpx;
  background: linear-gradient(#FDF8E4, #FEFEFC);
}
.cur_yue{
  width: 750rpx;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: relative;
  top: 30rpx;
}
.cur_yue view{
  color: rgb(141, 135, 117);
  font-size: 30rpx;
}
.cur_yue image{
  width: 23rpx;
  margin-left: 13rpx;
}
.head_paihang_view{
  width: 750rpx;
  height: 330rpx;
  position: absolute;
  left: 0rpx;
  bottom: 65rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: flex-end;
}
.head_paihang_item{
  width: 170rpx;
  height: 330rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
}
.icon_gongxianzhi{
  color: rgb(141, 135, 117);
  font-size: 28rpx;
  height: 25rpx;
  line-height: 25rpx;
  letter-spacing: 2rpx;
  margin-top: 20rpx;
}
.txt_gongxianzhi{
  color: rgb(0, 0, 0);
  font-size: 33rpx;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 20rpx;
  font-weight: 800;
}
.txt_nickname{
  width: 170rpx;
  overflow: hidden;
  color: rgb(35, 30, 25);
  font-size: 28rpx;
  margin-top: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.head_tx_view{
  width: 170rpx;
  height: 175rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  position: relative;
}
.head_tx_max{
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
  overflow: hidden;
  margin-bottom: -38rpx;
}
.head_tx_min{
  width: 130rpx;
  height: 130rpx;
  border-radius: 80rpx;
  overflow: hidden;
  margin-bottom: -38rpx;
}
.txt_paiming_max{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 25rpx;
  text-align: center;
  background-color: rgb(250, 154, 45);
  color: white;
}
.txt_paiming_min{
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  text-align: center;
  background-color: rgb(250, 154, 45);
  color: white;
}
.paihang_content{
  width: 690rpx;
  padding: 0 30rpx 0 30rpx;
  position: relative;
  background-color: white;
}
.paihang_item{
  width: 690rpx;
  height: 140rpx;
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-top: #EFEFEF solid 3rpx;
}
.txt_paihang_num{
  color: #000000;
  font-size: 30rpx;
  margin-right: 20rpx;
}
.paihang_tx{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.paihang_nickname{
  max-width: 350rpx;
  color: #000000;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.gongxian_view{
  width: 170rpx;
  height: 140rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  right: 0rpx;
}
.icon_gxz{
  color: rgb(145, 145, 145);
  font-size: 28rpx;
}
.txt_gxz{
  color: rgb(47, 47, 47);
  font-size: 30rpx;
}
.my_info{
  width: 690rpx;
  padding: 0 30rpx 0 30rpx;
  height: 120rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: rgb(239, 239, 239);
}
.my_tx{
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.img_sm{
  width: 750rpx;
  background-color: white;
}
.my_info_content{
  width: 500rpx;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}
.my_info_nickname{
  color: #000000;
  font-size: 28rpx;
}
.icon_mygxz{
  color: rgb(133, 133, 133);
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
}