view,
scroll-view,
swiper,
swiper-item,
movable-area,
movable-view,
cover-view,
cover-image,
icon,
text,
rich-text,
progress,
button,
checkbox-group,
checkbox,
form,
input,
label,
picker,
picker-view,
radio-group,
radio,
slider,
switch,
textarea,
navigator,
functional-page-navigator,
image,
video,
camera,
live-player,
live-pusher,
map,
canvas,
open-data,
web-view,
ad {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

page {
  height: 100%;
}

.container {
  height: 100%;
  background: #ffffff url('https://hnenjoy.oss-cn-shanghai.aliyuncs.com/zt_hbsjkh/renewal/bg.png') left -100rpx/100% auto no-repeat;
  overflow: auto;
  padding-bottom: 100rpx;
}

.container .vip_top {
  display: flex;
  flex-direction: column;
  padding-left: 37rpx;
  padding-top: 30rpx;
}

.container .vip_top .user_info {
  flex-shrink: 0;
  display: flex;
  align-items: center;
}

.container .vip_top .user_info .touxiang {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  margin-right: 12rpx;
  background: #ffffff;
}

.container .vip_top .user_info .name-info {
  margin-right: 12rpx;
  display: flex;
  flex-direction: column;
  line-height: 1;
}

.container .vip_top .user_info .nickname {
  font-size: 30rpx;
  color: #FFE2B2;
}

.icon_vip-wrap {
  position: relative;
}

.pt-vip-text {
  position: absolute;
  left: 0;
  right: 0;
  color: #FFE2B2;
  font-size: 20rpx;
  text-align: center;
  top: 0rpx;
}

.vip-text {
  position: absolute;
  left: 0;
  right: 0;
  font-size: 20rpx;
  text-align: center;
  top: 0rpx;
}

.container .vip_top .user_info .icon_vip {
  width: 100rpx;
  position: relative;
  display: block;
}

.vip-detail {
  padding: 80rpx 20rpx;
}

.vip-detail .title {
  padding: 0 30rpx;
  margin-bottom: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.vip-detail .title .name {
  color: #030303;
  font-weight: bold;
  font-size: 30rpx;
}

.vip-detail .title .auto {
  color: #03030380;
  font-size: 22rpx;
}

.vip-detail .way {
  padding: 36rpx 28rpx;
  background: #FDEBEB;
}

.vip-detail .way .way-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.vip-detail .way .way-item:not(:last-child) {
  margin-bottom: 32rpx;
}

.vip-detail .way .way-item .name {
  color: #030303;
  font-size: 30rpx;
}

.vip-detail .way .way-item .value {
  color: #030303bb;
  font-size: 26rpx;
}

.container .btn {
  display: flex;
  justify-content: center;
}

.container .btn text {
  width: 650rpx;
  height: 86rpx;
  background: #fb6b6a;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 44rpx;
  color: #ffffff;
}

.empty-tip {
  text-align: center;
  margin-top: 200rpx;
  color: #cccccc;
}

.tip-dialog {
  position: relative;
  width: 560rpx;
}

.tip-dialog .mask {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: #00000060;
}

.tip-dialog-wrapper {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tip-dialog-content {
}

.tip-dialog-content .dialog-head-img {
  width: 560rpx;
  height: 200rpx;
}

.tip-dialog-content .dialog-head-img image {
  width: 100%;
  height: 100%;
}

.tip-dialog-content .dialog-close {
  position: absolute;
  bottom: -110rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 84rpx;
  height: 84rpx;
}

.tip-dialog-content .content {
  width: 560rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 40rpx 40rpx 30rpx;
}

.tip-dialog-content .content-info {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.tip-dialog-content .info-text {
  font-family: MiSans;
  font-weight: 500;
  font-size: 30rpx;
  color: #000;
  line-height: 48rpx;
}

.tip-dialog-content .info-text image {
  width: 70rpx;
  height: 70rpx;
  vertical-align: middle;
  margin-right: 33rpx;
}

.tip-dialog-content .code-box {
  display: flex;
  margin-top: 12rpx;
  position: relative;
  width: 479rpx;
  height: 71rpx;
}

.tip-dialog-content .code-input {
  width: 479rpx;
  height: 71rpx;
  background: #fafafa;
  border: 1px solid #e6e6e6;
  font-weight: 500;
  font-size: 30rpx;
  color: #000;
}

.tip-dialog-content .get-code {
  position: absolute;
  top: 0;
  right: 0rpx;
  width: 200rpx;
  height: 71rpx;
  line-height: 71rpx;
  font-weight: normal;
  color: #fff;
  text-align: center;
  background: #457bf7;
}

.tip-dialog-content .cancel {
  width: 225rpx;
  height: 80rpx;
  background: #e6e6e6;
  color: #000;
  font-size: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tip-dialog-content .confirm {
  width: 225rpx;
  height: 80rpx;
  background: #ffd400;
  color: #000;
  font-size: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tip-dialog-content .btn-box {
  width: 100%;
  margin-top: 70rpx;
  font-size: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.tip-dialog-content .confirm-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 480rpx;
  height: 80rpx;
  margin-top: 70rpx;
  background: #ffd400;
  font-weight: 400;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0.1em;
}

.login-dialog {
  position: relative;
}

.login-dialog .mask {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: #00000060;
}

.login-dialog-wrapper {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
  display: flex;
  align-items: center;
  justify-content: center;
}

.login-dialog-content {
  background: #ffffff;
  width: 610rpx;
  padding: 40rpx 40rpx;
  border-radius: 16rpx;
}

.login-dialog-content .title {
  text-align: center;
  margin-bottom: 40rpx;
  font-size: 36rpx;
}

.login-dialog-content .phone {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
}

.login-dialog-content .code {
  display: flex;
  align-items: center;
  margin-bottom: 50rpx;
}

.login-dialog-content .code .label {
  flex-shrink: 0;
}

.login-dialog-content .code input {
  background: #eee;
  height: 70rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
}

.login-dialog-content .code .getCode {
  font-size: 24rpx;
  flex-shrink: 0;
  background: #1AAD19;
  height: 70rpx;
  width: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 35rpx;
  color: #ffffff;
}

.login-dialog-content .submit {
  text-align: center;
}

.login-dialog-content .submit button {
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
}